                    {
                        "type": "string",
                        "description": "Kvm Name",
                        "name": "name",
                        "in": "path",
                        "required": true
                    }
                ],
                "responses": {
                    "200": {
                        "description": "code = success? 1 : 0",
                        "schema": {
                            "$ref": "#/definitions/_domain.Response"
                        }
                    }
                }
            }
        },
