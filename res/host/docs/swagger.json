        "/api/v1/vnc/getToken": {
            "get": {
                "consumes": [
                    "application/json"
                ],
                "produces": [
                    "application/json"
                ],
                "summary": "根据VNC Port获取Token",
                "parameters": [
                    {
                        "type": "string",
                        "description": "VNC Port",
                        "name": "port",
                        "in": "query",
                        "required": true
                    }
                ],
                "responses": {
                    "200": {
                        "description": "code = success? 1 : 0",
                        "schema": {
                            "allOf": [
